	"version": "0.2.0",
	"configurations":
	[
		{
			"name": "Debug Dragon 32 w/ Dragon Sprites",
			"type": "cppvsdbg",
			"request": "launch",
			"args":
			[
				"dragon32",
				"-ext",
				"multi",
				"-ext:multi:slot1",
				"sprites",
				"-flop1",
				"${workspaceFolder}\\Burger.vdk",
				"-debug",
				"-autoboot_delay",
				"3",
				"-autoboot_command",
				"RUN \"BOOT\"\\n"
			],
			"program": "e:/mame/mame.exe",
			"cwd": "e:/mame"
		},
		{
			"name": "Debug CoCo w/ WordPak2+",
			"type": "cppvsdbg",
			"request": "launch",
			"args":
			[
				"coco2",
				"-ext",
				"multi",
				"-ext:multi:slot1",
				"wpk2p",
				"-flop1",
				"${workspaceFolder}\\Burger.dsk",
				"-debug",
				"-autoboot_delay",
				"2",
				"-autoboot_command",
				"LOAD \"BOOT\",R\\n"
			],
			"program": "e:/mame/mame.exe",
			"cwd": "e:/mame"
		},
		{
			"name": "Debug Dragon 32 w/ SuperSprite",
			"type": "cppvsdbg",
			"request": "launch",
			"args":
			[
				"dragon32",
				"-ext",
				"multi",
				"-ext:multi:slot1",
				"ssfm",
				"-flop1",
				"${workspaceFolder}\\Burger.vdk",
				"-debug",
				"-autoboot_delay",
				"3",
				"-autoboot_command",
				"RUN \"BOOT\"\\n"
			],
			"program": "e:/mame/mame.exe",
			"cwd": "e:/mame"
		}
	]
}