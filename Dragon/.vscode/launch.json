	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Project",
			"type": "cppvsdbg",
			"request": "launch",
			"args" : [
				"dragon32",
				"-ext",
				"multi",
				"-ext:multi:slot1",
				"sprites",
				"-flop1",
				"${workspaceFolder}\\Burger.vdk",
				"-debug",
				"-autoboot_delay",
				"3",
				"-autoboot_command",
				"RUN \"BOOT\"\\n"
			],
			"program": "e:/mame/mame.exe",
			"cwd": "e:/mame"
		}
	]
}