	"version": "0.2.0",
	"configurations":
	[
		{
			"name": "Debug Disk on EZ CGI card",
			"type": "cppvsdbg",
			"request": "launch",
			"args":
			[
				"apple2e",
				"-sl7",
				"ezcgi",
				"-flop1",
				"${workspaceFolder}\\Burger.dsk",
				"-debug"
			],
			"program": "e:/mame/mame.exe",
			"cwd": "e:/mame"
		},
		{
			"name": "Debug Disk on Super Sprite card",
			"type": "cppvsdbg",
			"request": "launch",
			"args":
			[
				"apple2e",
				"-sl7",
				"ssprite",
				"-flop1",
				"${workspaceFolder}\\Burger.dsk",
				"-debug"
			],
			"program": "e:/mame/mame.exe",
			"cwd": "e:/mame"
		}
	]
}