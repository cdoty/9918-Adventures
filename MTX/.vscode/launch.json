	"version": "0.2.0",
	"configurations":
	[
		{
			"name": "Debug MTX",
			"type": "cppvsdbg",
			"request": "launch",
			"args":
			[
				"mtx500",
				"-cass",
				"${workspaceFolder}\\BurgerMTX.wav",
				"-debug",
				"-autoboot_delay",
				"1",
				"-autoboot_command",
				"LOAD \"\"\\n"
			],
			"program": "e:/mame/mame.exe",
			"cwd": "e:/mame"
		},
		{
			"name": "Debug Cartridge",
			"type": "cppvsdbg",
			"request": "launch",
			"args":
			[
				"mtx500",
				"-cart",
				"${workspaceFolder}\\BurgerMTX.wav",
			],
			"program": "e:/mame/mame.exe",
			"cwd": "e:/mame"
		}
	]
}