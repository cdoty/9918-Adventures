	"version": "0.2.0",
	"configurations":
	[
		{
			"name": "Debug Cassette",
			"type": "cppvsdbg",
			"request": "launch",
			"args":
			[
				"pk8002",
				"-cass",
				"${workspaceFolder}\\BurgerPK8002.wav",
				"-debug",
				"-autoboot_command",
				"RBURGER\\n"
			],
			"program": "e:/mame/mame.exe",
			"cwd": "e:/mame"
		}
	]
}